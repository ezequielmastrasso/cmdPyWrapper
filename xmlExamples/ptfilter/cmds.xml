<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- brickMake modes and options to convert and existing ptc 
	 file into tdl format
	1 file out per file in
-->
<cmdsRoot init_xmlCmd_rev="1">

	<ptfilter>
	<cmdConfig>
	  <inputFile>True</inputFile>
	  <cmdPath>c:\programFiles\prman\bin</cmdPath>
	  <cmdFile>ptfilter.exe</cmdFile>
	  <multipleinputFiles>True</multipleinputFiles>
	  <outputFile>True</outputFile>
	  <multipleOutputFiles>True</multipleOutputFiles>
	  <outputFileExtension>ptc</outputFileExtension>
	</cmdConfig>
	<cmdOptions>
	  <filter>
	  		<state>False</state>
		    <type>fixedString</type>
		    <param>
			    	<mode>ssdiffusion</mode>
			    	<mode>occlusion</mode>
			    	<mode>colorbleeding</mode>
			    	<mode>photonmap</mode>
			    	<mode>sort</mode>
			    	<mode>crop</mode>	    
		    </param>
	  </filter>
	  <material>
	  		<state>False</state>
		    <type>fixedString</type>
		    <param>
			    	<mode>apple</mode>
			    	<mode>chicken1</mode>
			    	<mode>chicken2</mode>
			    	<mode>ketchup</mode>
			    	<mode>marble</mode>
			    	<mode>skimmilk</mode>
			    	<mode>wholemilk</mode>
			    	<mode>cream</mode>
			    	<mode>skin1</mode>
			    	<mode>skin2</mode>
			    	<mode>spectralon</mode>
		    </param>
	  </material>
	  <partial>
	  		<state>False</state>
			<type>Boolean</type>
			<param>
				<mode>True</mode>
				<mode>False</mode>
			</param>
	  </partial>
	  <organize>
	  		<state>False</state>
			<type>Boolean</type>
			<param>
				<mode>True</mode>
				<mode>False</mode>
			</param>
	  </organize>
	  <unitlength>
	  		<state>False</state>
			<type>Float</type>
			<param>
				<mode>1.0</mode>
			</param>
	  </unitlength>
	  <scattering>
	  		<state>True</state>
			<type>Float3</type>
			<param>
				<mode>1.0</mode>
				<mode>2.0</mode>
				<mode>3.0</mode>
			</param>
	  </scattering>
	  <absorption>
	  		<state>True</state>
			<type>Float3</type>
			<param>
				<mode>1.0</mode>
				<mode>2.0</mode>
				<mode>3.0</mode>
			</param>
	  </absorption>
	  <albedo>
	  		<state>True</state>
			<type>Float3</type>
			<param>
				<mode>1.0</mode>
				<mode>2.0</mode>
				<mode>3.0</mode>
			</param>
	  </albedo>
	  <diffusemeanfreepath>
	  		<state>True</state>
			<type>Float3</type>
			<param>
				<mode>1.0</mode>
				<mode>2.0</mode>
				<mode>3.0</mode>
			</param>
	  </diffusemeanfreepath>
	  <ior>
	  		<state>False</state>
			<type>Float</type>
			<param>
				<mode>1.0</mode>
			</param>
	  </ior>
	  <smooth>
	  		<state>False</state>
			<type>Boolean</type>
			<param>
				<mode>True</mode>
				<mode>False</mode>
			</param>
	  </smooth>
	  <areachannel>
	  		<state>False</state>
			<type>fixedString</type>
		    <param>
			    	<mode>_area</mode>
			</param>
	  </areachannel>
	  <radiancechannel>
	  		<state>False</state>
			<type>string</type>
		    <param>
			    	<mode>_radiance</mode>
			</param>
	  </radiancechannel>
	  <radiositychannel>
	  		<state>False</state>
			<type>string</type>
		    <param>
			    	<mode>_radiosity</mode>
			</param>
	  </radiositychannel>
	  <maxsolidangle>
	  		<state>False</state>
			<type>Float</type>
			<param>
				<mode>0.07</mode>
			</param>
	  </maxsolidangle>
	  <samplebase>
	  		<state>False</state>
			<type>Integer</type>
			<param>
				<mode>1</mode>
			</param>
	  </samplebase>
	  <bias>
	  		<state>False</state>
			<type>Float</type>
			<param>
				<mode>.07</mode>
			</param>
	  </bias>
	  <sides >
	  		<state>False</state>
			<type>fixedString</type>
		    <param>
			    	<mode>front</mode>
			    	<mode>back</mode>
			    	<mode>both</mode>
			</param>
	  </sides >
	  <clamp>
	  		<state>False</state>
			<type>Boolean</type>
			<param>
				<mode>True</mode>
				<mode>False</mode>
			</param>
	  </clamp>
	  <sortbleeding>
	  		<state>False</state>
			<type>Boolean</type>
			<param>
				<mode>True</mode>
				<mode>False</mode>
			</param>
	  </sortbleeding>
	  <envdirs>
	  		<state>False</state>
			<type>Float</type>
			<param>
				<mode>1.0</mode>
			</param>
	  </envdirs>
	  <maxdist>
	  		<state>False</state>
			<type>Float</type>
			<param>
				<mode>100.0</mode>
			</param>
	  </maxdist>
	  <falloff>
	  		<state>False</state>
			<type>Float</type>
			<param>
				<mode>100.0</mode>
			</param>
	  </falloff>
	  <falloffmode>
	  		<state>False</state>
			<type>Float</type>
			<param>
				<mode>1.0</mode>
			</param>
	  </falloffmode>
	 <bounces>
	  		<state>False</state>
			<type>Integer</type>
			<param>
				<mode>1</mode>
			</param>
	  </bounces>
	  <distribution>
	  		<state>False</state>
			<type>fixedString</type>
		    <param>
			    	<mode>cosine</mode>
			    	<mode>uniform</mode>
			</param>
	  </distribution>
	  <coneangle>
	  		<state>False</state>
			<type>Float</type>
			<param>
				<mode>1.0</mode>
			</param>
	  </coneangle>
	  <nphotons>
	  		<state>False</state>
			<type>Integer</type>
			<param>
				<mode>1</mode>
			</param>
	  </nphotons>
	  <progress>
	  		<state>False</state>
			<type>fixedString</type>
			<param>
				<mode>0</mode>
				<mode>1</mode>
				<mode>2</mode>
			</param>
	  </progress>
	  
	  
	</cmdOptions>
	</ptfilter>
	

</cmdsRoot>