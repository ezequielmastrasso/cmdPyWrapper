					    <type>string</type>
					    <param>
						    	<mode></mode>    
					    </param>
				  </forcecolor>
				  <gamma>
				  		<state>False</state>
					    <type>Float</type>
					    <param>
						    	<mode>1.0</mode>
					    </param>
				  </gamma>
				  <rgbagamma>
				  		<state>False</state>
					    <type>string</type>
					    <param>
						    	<mode>1.0 1.0 1.0 1.0</mode>    
					    </param>
				  </rgbagamma>
				  <colorspace>
