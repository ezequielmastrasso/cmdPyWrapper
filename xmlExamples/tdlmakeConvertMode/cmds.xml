						    	<mode>catmull-rom</mode>
						    	<mode>bessel</mode>
						    	<mode>mitchell</mode>
						    	<mode>sinc</mode>
					    </param>
				  </filter>
				  <window>
				  		<state>False</state>
					    <type>fixedString</type>
