<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- tdlMake modes and options to convert and existing file
	texture into tdl format
	1 file out per file in mode
	
-->
<cmdsRoot init_xmlCmd_rev="1">

	<tdlMakeConvertMode>	
			<cmdConfig>
				  <inputFile>True</inputFile>
				  <cmdPath>C:\Program Files\3Delight\bin</cmdPath>
				  <cmdFile>tdlmake.exe</cmdFile>
				  <multipleinputFiles>True</multipleinputFiles>
				  <outputFile>True</outputFile>
				  <multipleOutputFiles>True</multipleOutputFiles>
				  <outputFileExtension>tdl</outputFileExtension>
			</cmdConfig>

			<cmdOptions>
	
				  <dirtex>
				  		<state>False</state>
					    <type>string</type>
					    <param>
						    	<mode></mode>    
					    </param>
				  </dirtex>
				  <shadow>
				  		<state>False</state>
					    <type>string</type>
					    <param>
						    	<mode></mode>    
					    </param>
				  </shadow>
